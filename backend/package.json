{
  "name": "pvhome-database",
  "version": "2.2.0",
  "description": "Swagger-UI and API Rest Routing for Open API v3.",
  "main": "dist/index.js",
  "scripts": {
    "build": "rm -rf dist/ && tsc && cp -a src/api docs ./dist/",
    "install-frontend": "cd ../frontend && npm install",
    "test": "/usr/bin/env NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=0  jest DashboardController.test --coverage --detectOpenHandles --runInBand  --config jest.config.js ",
    "watch": "tsc -w",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run build",
    "jest": "/usr/bin/env NODE_ENV=test NODE_TLS_REJECT_UNAUTHORIZED=0  jest --coverage --detectOpenHandles --runInBand  --config jest.config.js ",
    "refresh": "npm uninstall ../frontend/ && npm install ../frontend/",
    "generate": "npx fern-api generate "
  },
  "exports": {},
  "author": {
    "name": "Leo Martins",
    "email": "lmartins@pontusvision.com"
  },
  "license": "MIT",
  "keywords": [
    "api",
    "expressjs",
    "middleware",
    "swagger",
    "openapi 3"
  ],
  "files": [
    "dist"
  ],
  "dependencies": {
