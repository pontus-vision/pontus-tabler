    "lodash": "^4.17.15",
    "morgan": "^1.9.1",
    "multer": "^1.4.2",
    "parseurl": "^1.3.3",
    "path-to-regexp": "^3.2.0",
    "pontus-node-jdbc": "file:./pontus-node-jdbc/dist",
    "pontus-tabler": "file:../../frontend",
